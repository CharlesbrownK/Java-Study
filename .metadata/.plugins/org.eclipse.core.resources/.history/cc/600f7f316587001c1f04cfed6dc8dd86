package org.opentutorials.javatutorials.operator;

public class ArithmeticDemo {

	public static void main(String[] args) {

		int result = 1 + 2;
		System.out.println(result);
		
		result = result - 1; // result -= reslt
		System.out.println(result);
		
		result = result * 3;
		System.out.println(result);
		
		result = result / 2;
		System.out.println(result);
		
		result = result + 10; // result += 10
		System.out.println(result);
		
		result = result % 7;
		System.out.println(result);
	}

}
